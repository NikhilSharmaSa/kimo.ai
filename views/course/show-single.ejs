<% layout("/layouts/boilerplate") %>
<body>
   
    <div class="container">
        <div class="row justify-content-md-center mt-5 mb-5">
          <div class="col col-md-6">
            <div class="card" style="width: 40rem;">
                <img src="https://images.unsplash.com/photo-1555967522-37949fc21dcb?w=500&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8Nnx8Ym9va3N8ZW58MHx8MHx8fDA%3D" class="card-img-top" alt="listing-image" style="width: 100%; height: 300px;">
                <form class="card-body">
                  <h3 class="card-title"><%= key.name  %></h3>
                  <p class="card-text"><%= key.description  %></p>
                  <h3 class="card-text">Domains </h3>
                 <div class="row"></div>
<% 

key.domain.forEach(element => {



  %>

  <button type="button" class="btn btn-primary"><%=
element %><span class="badge text-bg-secondary"></span>
</button>

  <%
});

%>
<h3 class="mt-5">Chapters</h3>
<%

let sn=0
key.chapters.forEach(element => {
  sn++
  %>
  <a href="/courses<%= key.id  %>/chapter<%=element.id %>/<%=sn %>" class="dist">
<div class="card mt-2">
  <div class="card-body">
   <b><%= sn%>.
   <%=
   
   element.name%></b> 
  </div>
</div>
</a>
<%
});

%>
                </form>
                 </div>

            
               
              </div>
          </div>
          
        </div>
     

    




       
</body>
